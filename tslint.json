{"extends": ["tslint:latest", "tslint-react"],
 "rules":{
     "semicolon": [true, "never"],
     "trailing-comma": [
	 true,
	 {
	     "multiline": {
		 "objects": "never",
		 "arrays": "never",
		 "functions": "never",
		 "typeLiterals": "ignore"
	     }
	 }
     ],
     "no-console": [false],
     "no-var-requires": false,
     "ignoreDefinitionFiles":true,
     "no-unused-variable": true,
     "ordered-imports": false,
     "object-literal-sort-keys":false
 }
}
